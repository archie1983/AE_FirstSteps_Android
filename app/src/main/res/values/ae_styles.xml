<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AECustomButton" parent="android:Widget.Button">
        <item name="android:background">@drawable/my_nine_patch</item>
        <item name="android:textColor">#00FF00</item>
    </style>
    <style name="AECustomFontStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:capitalize">characters</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">12pt</item>
        <item name="android:textColor">#00FF00</item>/>
    </style>
    <style name="AECustomTheme" parent="AppTheme">
        <item name="android:buttonStyle">@style/AECustomButton</item>
        <item name="android:textColor">#00FFFF</item>
        <item name="android:colorPrimary">@color/ae_colorPrimary</item>
        <item name="android:colorPrimaryDark">@color/ae_colorPrimaryDark</item>
        <item name="android:colorAccent">@color/ae_colorAccent</item>
        <!-- The below is only supported on API 26 or higher -->
        <!--item name="android:fontStyle">@style/AECustomFontStyle</item-->
    </style>
</resources>